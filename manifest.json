{
	"name": "1024",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["*://*.xp303.info/*"],
			"css":["style.css"],
			"js": ["jquery.min.js","main.js","worker.js"],
			"all_frames": true,
			"run_at":"document_end"
		}
	],
	"permissions": [
        "contextMenus",
		"*://*.xp303.info/*"
	],

	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"browser_action": {
		"name": "1024",
		"default_icon": "icon_128.png"
	},
	"manifest_version": 2
}
